@model IEnumerable<SBC.Models.CoursesTest>

@{
    ViewBag.Title = "Index";
}

<h2>Предметы</h2>
<p>
    @Html.ActionLink("Добавить", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Наименование")
        </th>
        <th></th>
    </tr>
    @foreach (var t in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(@Html.DisplayFor(modelItem => t.NameCourses).ToString(), "Index", "Test", null, new { id = t.id })
            </td>
            <td>
                @Html.ActionLink("Удалить", "Delete", new {id = t.id})
            </td>
        </tr>
    }
</table>
<div>
    @Html.ActionLink("Назад", "Index")
</div>


