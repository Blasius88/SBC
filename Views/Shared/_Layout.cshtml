<!DOCTYPE html>
<html lang="ru">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <link href="~/Content/MySite.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
</head>
<body onmousedown="return false;" onclick="return true;" oncontextmenu="return false;">
    <!-- Заголовок -->
    <div class="header-strip">
        <!--Панель аутентификации-->
        <div class="container">
            @Html.Action("UserInfo", "Menu")
        </div> <!--Панель аутентификации (конец)-->
        <!-- панель навигации-->
        <div class="container">
            <div class="navbar navbar-inverse" role="navigation">
                <ul class="nav nav-pills">
                    @Html.Action("Main", "Menu", new
                    {
               c = Request.RequestContext.RouteData.Values["conoteoller"]
               })
                </ul>
            </div>
        </div><!--панель навигации (конец)-->
    </div><!--Заголовок (конец)-->
    <!--Содержимое сайта-->
    <div class="main-body">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-4 col-md-offset-2">
                    <h2>@ViewBag.MyText</h2>
                </div>
            </div>
            <div class="row">
                <!--Левая боковая панель-->
                <div class="col-md-2">
                    @RenderSection("SidePanel", required: false)
                </div>
                <!--/Левая боковая панель (конец)-->
                <!--Основная панель-->
                <div class="col-md-10">

                    @RenderBody()

                </div>
                <!--/Основная панель (конец)-->
            </div>
        </div>
    </div>
    <!--Содержимое сайта (конец)-->
    <!--Подвал страницы-->
    <div class="footer-strip">
        <div class="container">
            <div class="row">
                @Html.Action("Map", "Menu")
            </div>
        </div>
    </div>
    <!--Подвал страницы (конец)-->
</body>
</html>
