<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/MySite.css" rel="stylesheet" />
</head>
<body>
    <!-- Заголовок -->
    <div class="header-strip">
        <!--Панель аутентификации-->
        <div class="container">
            @Html.Action("UserInfo", "Menu")
        </div> <!--Панель аутентификации (конец)-->
        <!-- панель навигации-->
        <div class="container">
            <div class="navbar navbar-inverse" role="navigation">
                <ul class="nav nav-pills">
                    @Html.Action("Main", "Menu", new
                    {
               c = Request.RequestContext.RouteData.Values["conoteoller"]
               })
                </ul>
            </div>
        </div><!--панель навигации (конец)-->
    </div><!--Заголовок (конец)-->
    <!--Содержимое сайта-->
    <div class="main-body">
        <div class="container">
            <div class="row">
                <div class="col-md-4 col-md-offset-2">
                    <h2>@ViewBag.MyText</h2>
                </div>
            </div>
            <div class="row">
                <!--Левая боковая панель-->
                <div class="col-md-3">
                    @RenderSection("SidePanel", required: false)
                </div>
                <!--/Левая боковая панель (конец)-->
                <!--Основная панель-->
                <div class="col-md-8">
                    @RenderBody()
                </div>
                <!--/Основная панель (конец)-->
            </div>
        </div>
    </div>
    <!--Содержимое сайта (конец)-->
    <!--Подвал страницы-->
    <div class="footer-strip">
        <div class="container">
            <div class="row">
                @Html.Action("Map", "Menu")
            </div>
        </div>
    </div>
    <!--Подвал страницы (конец)-->
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/jquery-3.5.1.js"></script>
</body>
</html>
